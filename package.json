{
  "name": "ad-request-form",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.js",
  "scripts": {
    "build": "webpack --display-error-details",
    "deploy": "npm test && git push heroku master",
    "postinstall": "npm run build",
    "webpack-watch": "webpack --watch",
    "start": "node server/server.js",
    "test": "npm run test:client && npm run test:server",
    "nodemon": "nodemon --ignore 'client/*.js' --ignore 'public/js/bundle.js' --ignore 'node_modules' server/server.js",
    "dev": "concurrently --kill-others \"npm run webpack-watch\" \"npm run nodemon\"",
    "test:client": "mocha test/client --recursive --compilers js:babel-register",
    "test:server": "mocha test/server --recursive --compilers js:babel-register"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-rewire": "^1.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "coffee-script": "^1.11.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-validator": "^2.20.10",
    "jquery": "^3.1.1",
    "morgan": "^1.7.0",
    "node-sass-middleware": "^0.9.8",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-dropzone": "^3.6.0",
    "react-router": "^2.8.1",
    "superagent": "^2.3.0",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "concurrently": "^2.2.0"
  },
  "engines": {
    "node": "6.1.0",
    "concurrently": "^2.2.0"
  }
}
